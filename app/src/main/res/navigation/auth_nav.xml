<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/auth_nav"
    app:startDestination="@+id/authFragment">
    <fragment
        android:id="@+id/authFragment"
        android:name="com.carlosvaldes.viajerum.views.auth.fragments.AuthFragment"
        android:label="fragment_auth">
        <action
            android:id="@+id/action_authFragment_to_loginFragment"
            app:destination="@+id/loginFragment" />
        <action
            android:id="@+id/action_authFragment_to_registerFragment"
            app:destination="@+id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.carlosvaldes.viajerum.views.auth.fragments.LoginFragment"
        android:label="fragment_login">
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.carlosvaldes.viajerum.views.auth.fragments.RegisterFragment"
        android:label="fragment_register">
    </fragment>
</navigation>